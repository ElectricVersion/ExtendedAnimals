{
  "multipart": [
    {   "when": { "facing": "up" },
      "apply": { "model": "minecraft:block/oak_fence_post"}
    },
    {   "when": { "facing": "north" },
      "apply": { "model": "minecraft:block/oak_fence_post", "x": 90}
    },
    {   "when": { "facing": "east" },
      "apply": { "model": "minecraft:block/oak_fence_post", "x": 90, "y": 90}
    },
    {   "when": { "facing": "up", "north": "true"},
      "apply": { "model": "eanimod:block/post_fence_oak", "uvlock": true , "y": 180}
    },
    {   "when": { "facing": "up", "east": "true"},
      "apply": { "model": "eanimod:block/post_fence_oak", "uvlock": true , "y": 270 }
    },
    {   "when": { "facing": "up", "south": "true"},
      "apply": { "model": "eanimod:block/post_fence_oak", "uvlock": true , "y": 0 }
    },
    {   "when": { "facing": "up", "west": "true"},
      "apply": { "model": "eanimod:block/post_fence_oak", "uvlock": true , "y": 90 }
    },
    {   "when": { "facing": "north", "north": "true"},
      "apply": { "model": "eanimod:block/post_extension_oak", "x": 90 }
    },
    {   "when": { "facing": "north", "south": "true"},
      "apply": { "model": "eanimod:block/post_extension_oak", "x": 270 }
    },
    {   "when": { "facing": "north", "up": "true"},
      "apply": { "model": "eanimod:block/post_connector_oak"}
    },
    {   "when": { "facing": "north", "down": "true"},
      "apply": { "model": "eanimod:block/post_connector_oak", "x": 180 }
    },
    {   "when": { "facing": "east", "east": "true"},
      "apply": { "model": "eanimod:block/post_extension_oak", "x": 90 , "y": 90 }
    },
    {   "when": { "facing": "east", "west": "true"},
      "apply": { "model": "eanimod:block/post_extension_oak", "x": 90 , "y": 270 }
    },
    {   "when": { "facing": "east", "up": "true"},
      "apply": { "model": "eanimod:block/post_connector_oak"}
    },
    {   "when": { "facing": "east", "down": "true"},
      "apply": { "model": "eanimod:block/post_connector_oak", "x": 180 }
    }
  ]
}